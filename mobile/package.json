    "@solana/mobile-wallet-adapter-protocol": "^2.0.0",
    "react-native-paper": "^5.12.0",
    "react-native-vector-icons": "^10.0.3",
    "react-native-chart-kit": "^6.12.0",
    "victory-native": "^37.0.2",
